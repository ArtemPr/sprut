                    {% if data.auth_list|default(false) %}
                        {% if data.auth_list['program']['view']|default(false) %}
                            checked
                        {% endif %}
                    {% endif %}
                   data-selected-by="workflow">{{ operations.auth_program }}
        </label>
        <ul class="hierarchy__child-scopes">
            <li class="js-check-scope-container" data-scope-for="repo:status">
                <label class="hierarchy__scope">
                    <input type="checkbox" name="role[program][add]" value="1"
                        {% if data.auth_list|default(false) %}
                            {% if data.auth_list['program']['add']|default(false) %}
                                checked
                            {% endif %}
                        {% endif %}
                           class="form-check-input js-checkbox-scope"/> создание
                </label>
