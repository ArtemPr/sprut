<ul class="hierarchy__group-list">
    <li class="hierarchy__group-item" data-scope-for="desktop">
        <label class="hierarchy__scope">
            <input type="checkbox" name="role[desktop]" value="desktop"
                   {% if data.auth_list|default(false) %}
                       {% if data.auth_list['desktop']|default(false) %}
                           checked
                       {% endif %}
                   {% endif %}
                   class="form-check-input js-checkbox-scope parent-checkbox-scope"
                   data-selected-by="workflow">{{ operations.auth_desktop }}
        </label>
    </li>
    <li class="hierarchy__group-item" data-scope-for="desktop">
        <label class="hierarchy__scope hierarchy__scope--parent">
            <input type="checkbox" name="role[program][view]" value="1"
