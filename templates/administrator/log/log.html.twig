{% extends 'base.html.twig' %}

{% block title %}СПРУТ 1.0{% endblock %}

{% block stylesheets %}
    {{ encore_entry_link_tags('app') }}
{% endblock %}

{% block body %}
    {{ include('block/header.html.twig') }}
    <div class="page page-layout">
        <div class="page-wrapper">
            <div class="container-fluid">
                    {% include 'administrator/block/header_list.html.twig' with {'header': 'Журнал событий', 'onpage_link': path('administrator_log')} %}
            </div>
            <div class="page-body">
                <div class="container-fluid">
                    <div class="row row-deck row-cards">
                        <div class="col-12">
                            <div class="card">
                                {% include 'administrator/user/block/search_panel.html.twig' with {'close_delete': true, 'close_edit': true, 'close_create': true} %}
                                <div class="ajax-table">
                                    {% include '/administrator/log/log_table.html.twig' %}
                                </div>
                            </div>
                            <div id="userEditingOverlay" class="offcanvas-backdrop fade"></div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

{% endblock %}
